{
  "name": "svelte4django",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "delete": "del --force mysite/statics/assets",
    "dev": "vite",
    "build": "npm run delete && vite build -c vite.production.js && npm run move",
    "move": "python mvindex.py",
    "serve": "vite preview",
    "watch": "python mvindex.py debug & vite build --watch -c vite.debug.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.11",
    "autoprefixer": "^10.3.7",
    "cssnano": "^5.0.8",
    "del-cli": "^4.0.1",
    "postcss": "^8.3.9",
    "postcss-load-config": "^3.1.0",
    "svelte": "^3.37.0",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.16",
    "vite": "^2.5.4"
  }
}
